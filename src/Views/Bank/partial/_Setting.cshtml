@model List<ICICI.AppCode.Reops.Entities.BankSetting>
@{
    int sr = 0;
    foreach (var item in Model)
    {
        sr++;
        <tr data-setting-id="@item.Id">
            <td>@sr</td>
            <td class="text-ellipsis"><span class="btn-edit btn-link cursor-pointer">@item.ProjectName</span></td>
            <td>@item.BaseUrl</td>
            <td class="text-ellipsis"><span data-toggle="tooltip" title="@item.APIKey">@item.APIKey</span></td>
            <td>@item.AccountNo</td>
            <td>@item.Password</td>
            <td>@item.CustomerId</td>
            <td>@item.Duration</td>
            <td>@item.IsActive</td>
        </tr>
    }
}
<script>
    $('[data-toggle="tooltip"]').tooltip();
    $('.btn-edit').click(e => dialog.bankSetting($(e.currentTarget).closest('tr').data().settingId));
</script>